    <!--Full width header End-->
  </div>
</section>
<section>
  <div class="owl-carousel owl-theme heroslider">
    <div class="item">
      <%= video_tag("video1.mov", autoplay: true, loop: true, muted: true, type: "video/mp4") %>							
    </div>
    <div class="item">
